      "cell_type": "code",
      "source": [
        "import numpy as np\n",
        "import pandas as pd\n",
        "import matplotlib.pyplot as plt\n",
        "import seaborn as sns\n",
        "from sklearn.model_selection import train_test_split, GridSearchCV\n",
        "from sklearn.metrics import accuracy_score\n",
        "from sklearn.linear_model import LogisticRegression\n",
        "from sklearn.ensemble import RandomForestClassifier, GradientBoostingClassifier\n",
        "from sklearn.tree import DecisionTreeClassifier\n",
        "import catboost\n",
        "import lightgbm\n",
        "import xgboost"
      ],
      "metadata": {
        "id": "RGjCbFAuKPLi"
      },
      "execution_count": 6,
      "outputs": []
    },
    {
      "cell_type": "code",
      "execution_count": null,
      "metadata": {
        "id": "JSQZby3s8viq"
      },
      "outputs": [],
      "source": [
        "# Load the data\n",
        "df = pd.read_csv('heart-nums.csv')\n",
        "\n",
        "# Split the data into training and test sets\n",
        "X = df.drop('target', axis=1)\n",
        "y = df['target']\n",
        "\n",
        "X_train, X_test, y_train, y_test = train_test_split(X, y, test_size=0.25, random_state=42)\n",
        "\n",
        "# Define the models\n",
        "models = {}\n",
        "models['Logistic Regression'] = LogisticRegression()\n",
        "models['Random Forest Classifier'] = RandomForestClassifier()\n",
        "models['Decision Tree Classifier'] = DecisionTreeClassifier()\n",
        "models['CatBoost Classifier'] = catboost.CatBoostClassifier()\n",
        "models['XGBoost Classifier'] = xgboost.XGBClassifier()\n",
        "models['LightGBM Classifier'] = lightgbm.LGBMClassifier()\n",
        "\n",
        "# Define the hyperparameter grids\n",
        "param_grids = {}\n",
        "param_grids['Logistic Regression'] = {\n",
        "    'C': [0.01, 0.1, 1, 10, 100],\n",
        "    'penalty': ['l1', 'l2'],\n",
        "}\n",
        "param_grids['Random Forest Classifier'] = {\n",
        "    'n_estimators': [100, 200, 500, 1000],\n",
        "    'max_depth': [3, 5, 10],\n",
        "}\n",
        "param_grids['Decision Tree Classifier'] = {\n",
        "    'max_depth': [3, 5, 10],\n",
        "}\n",
        "param_grids['CatBoost Classifier'] = {\n",
        "    'iterations': [100, 200, 500],\n",
        "    'learning_rate': [0.01, 0.1, 0.5],\n",
        "}\n",
        "param_grids['XGBoost Classifier'] = {\n",
        "    'n_estimators': [100, 200, 500, 1000],\n",
        "    'max_depth': [3, 5, 10],\n",
        "}\n",
        "param_grids['LightGBM Classifier'] = {\n",
        "    'n_estimators': [100, 200, 500, 1000],\n",
        "    'max_depth': [3, 5, 10],\n",
        "}\n",
        "\n",
        "# Tune the models using GridSearchCV\n",
        "best_models = {}\n",
        "for model_name, model in models.items():\n",
        "    grid_search = GridSearchCV(model, param_grids[model_name], cv=5, scoring='accuracy')\n",
        "    grid_search.fit(X_train, y_train)\n",
        "    best_models[model_name] = grid_search.best_estimator_\n",
        "\n",
        "# Evaluate the models on the test set\n",
        "predictions = {}\n",
        "for model_name, model in best_models.items():\n",
        "    predictions[model_name] = model.predict(X_test)\n",
        "\n",
        "accuracies = {}\n",
        "for model_name, predictions in predictions.items():\n",
        "    accuracies[model_name] = accuracy_score(y_test, predictions)\n",
        "\n",
        "# Display the results\n",
        "table = pd.DataFrame({'Model': models, 'Accuracy': accuracies})"
      ]
    },
